{
    "extends": "mpfe",
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-return-await": "off",
        "react/jsx-uses-react": "warn",
        "react/jsx-uses-vars": "warn"
    }
}